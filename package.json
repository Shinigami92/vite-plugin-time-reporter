{
  "name": "vite-plugin-time-reporter",
  "version": "1.1.0",
  "description": "Time reporting for Vite project",
  "scripts": {
    "clean": "rimraf dist pnpm-lock.yaml node_modules",
    "build": "tsc --noEmit && vite build",
    "format": "prettier --write .",
    "lint": "eslint .",
    "prepublishOnly": "pnpm run clean && pnpm install && pnpm run build"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "time",
    "reporting",
    "report",
    "plugin",
    "vitejs",
    "awesome"
  ],
  "author": {
    "name": "Christopher Quadflieg",
    "email": "chrissi92@hotmail.de",
    "url": "https://github.com/Shinigami92"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Shinigami92/vite-plugin-time-reporter.git"
  },
  "bugs": "https://github.com/Shinigami92/vite-plugin-time-reporter/issues",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "~18.8.3",
    "@typescript-eslint/eslint-plugin": "~5.39.0",
    "@typescript-eslint/parser": "~5.39.0",
    "eslint": "~8.25.0",
    "eslint-config-prettier": "~8.5.0",
    "eslint-define-config": "~1.7.0",
    "eslint-gitignore": "~0.1.0",
    "eslint-plugin-prettier": "~4.2.1",
    "prettier": "2.7.1",
    "prettier-plugin-organize-imports": "~3.1.1",
    "rimraf": "~3.0.2",
    "typescript": "~4.8.4",
    "vite": "~3.1.6",
    "vite-plugin-dts": "~1.6.5"
  },
  "peerDependencies": {
    "vite": "^2.0.0 || ^3.0.0"
  },
  "packageManager": "pnpm@7.13.2"
}
